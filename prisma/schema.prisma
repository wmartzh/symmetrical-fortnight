// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Users {
  id       String   @id @default(uuid()) @map("_id")
  email    String   @unique
  name     String?
  lastName String?
  address String?
  password String
  createdAt String
}